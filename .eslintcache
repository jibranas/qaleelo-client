\n        </div>\r\n      </Navbar>\r\n\r\n      <Transition\r\n        in={navOpen && props.entryStore}\r\n        timeout={300}\r\n        mountOnEnter\r\n        unmountOnExit\r\n      >\r\n        {(state) => {\r\n          {\r\n            /* if (state === \"exited\") setSubContainer(false); */\r\n          } //Code for NavBar to reset to main menu every time it closes\r\n          return (\r\n            <>\r\n              <QuizSidenav state={state} click={closeNav} />\r\n              <div\r\n                className=\"overlay\"\r\n                style={\r\n                  state === \"entering\"\r\n                    ? { animation: \"show .3s forwards\" }\r\n                    : state === \"entered\"\r\n                    ? { opacity: \"1\" }\r\n                    : { animation: \"show .3s backwards reverse\" }\r\n                }\r\n                onClick={closeNav}\r\n              ></div>\r\n              {\r\n                <div\r\n                  className=\"closeBtn\"\r\n                  style={\r\n                    state === \"entering\"\r\n                      ? { animation: \"show .3s forwards\" }\r\n                      : state === \"entered\"\r\n                      ? { opacity: \"1\" }\r\n                      : { animation: \"show .3s backwards reverse\" }\r\n                  }\r\n                  onClick={closeNav}\r\n                >\r\n                  &times;\r\n                </div> //This is code for a close button\r\n              }\r\n            </>\r\n          );\r\n        }}\r\n      </Transition>\r\n\r\n      <QuizBackgroundPage></QuizBackgroundPage>\r\n    </div>\r\n  );\r\n}\r\n","C:\\Users\\jibra\\Documents\\Projects\\Web Development Main\\myWebProjects\\lisanUlQuranApp\\quran-app\\src\\components\\ScoreCard.js",["897","898","899","900","901","902","903","904","905","906","907","908","909","910","911","912","913","914","915","916","917","918","919","920","921","922","923","924","925","926","927","928","92